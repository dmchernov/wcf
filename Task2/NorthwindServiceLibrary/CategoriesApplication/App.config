<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <customBinding>
                <binding name="NetHttpBinding_ICategoryService">
                    <binaryMessageEncoding />
                    <httpTransport maxReceivedMessageSize="2000000000" />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost/CategoryService" binding="customBinding"
                bindingConfiguration="NetHttpBinding_ICategoryService" contract="CategoryService.ICategoryService"
                name="NetHttpBinding_ICategoryService" />
        </client>
    </system.serviceModel>
</configuration>